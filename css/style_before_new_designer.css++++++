}

 nav.maniNav ul li a:hover{
    text-decoration:none;
	background: none repeat scroll 0 0 #FFB618;
	line-height:20px;
	color:#48697E;
	cursor: pointer;
	border-left: 1px solid #25292e;
	border-radius: 6px;
} 

